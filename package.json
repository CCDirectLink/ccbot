{
  "name": "crosscode-bot",
  "version": "0.0.1",
  "main": "./dist/main.js",
  "scripts": {
    "build": "webpack",
    "start": "npm run build && node --inspect ./dist/main.js",
    "watch": "nodemon --watch src -e ts --exec \"npm start\"",
    "test": "npm run lint && mocha -r ts-node/register tests/**/*.spec.ts",
    "lint": "eslint src/*.ts src/**/*.ts",
    "dist": "npm run test && webpack --config webpack.config.production.js"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.10",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.0.3",
    "eslint": "^5.16.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "@discordjs/uws": "^10.149.0",
    "bufferutil": "^4.0.1",
    "discord.js": "^11.5.1",
    "discord.js-commando": "^0.10.0"
  }
}
